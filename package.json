{
  "name": "spotify-karaoke",
  "version": "0.3.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "serve:pl": "ng serve --configuration=pl",
    "build": "ng build",
    "pre:build:prod": "node tools/prepare_secrets.js",
    "build:prod": "npm run pre:build:prod && ng build spotify --aot --buildOptimizer --extractLicenses --no-namedChunks --optimization --outputHashing=all --no-sourceMap --no-vendorChunk",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prepare": "husky install",
    "release": "release-it",
    "extract-i18n": "ng extract-i18n --output-path apps/spotify/src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/cdk": "^11.2.10",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "^11.2.0",
    "@angular/material": "^11.2.10",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@nrwl/angular": "12.0.8",
    "autoprefixer": "^10.2.5",
    "ngx-cookie-service": "^11.0.2",
    "ngx-google-analytics": "^11.2.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular-eslint/eslint-plugin": "~2.0.2",
    "@angular-eslint/eslint-plugin-template": "~2.0.2",
    "@angular-eslint/template-parser": "~2.0.2",
    "@angular/cli": "~11.2.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@angular/localize": "^11.2.14",
    "@nrwl/cli": "12.0.8",
    "@nrwl/cypress": "12.0.8",
    "@nrwl/eslint-plugin-nx": "12.0.8",
    "@nrwl/jest": "12.0.8",
    "@nrwl/linter": "12.0.8",
    "@nrwl/storybook": "^12.4.0",
    "@nrwl/tao": "12.0.8",
    "@nrwl/workspace": "12.0.8",
    "@release-it/conventional-changelog": "^2.0.1",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/angular": "^6.2.7",
    "@storybook/builder-webpack5": "^6.2.7",
    "@storybook/manager-webpack5": "^6.3.0",
    "@types/jest": "26.0.8",
    "@types/node": "^14.14.33",
    "@types/spotify-api": "0.0.8",
    "@types/spotify-web-playback-sdk": "^0.1.8",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "cypress": "^6.0.1",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^6.0.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.2",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.12",
    "prettier": "2.2.1",
    "release-it": "^14.6.1",
    "storybook-dark-mode": "^1.0.8",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.4"
